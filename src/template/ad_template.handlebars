<hr style="opacity: 0.02"/>
<button type="button" style="background-color: #c59b2f" class="mainbg radius" data-wwid="toggle-hidden">Ascunde</button>

<button style="vertical-align: middle; padding: 9px" data-wwid="temp-save">
    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
         fill="{{#if isTempSaved}}#cb2727{{else}}none{{/if}}"
         stroke="{{#if isTempSaved}}none{{else}}#333{{/if}}" stroke-width="1">
        <polygon points="12,2 15,10 23,10 17,14 19,22 12,17 5,22 7,14 1,10 9,10"/>
    </svg>
</button>

{{#if hasNoPhone}}
    <span>anuntul nu are nr telefon;</span>
{{/if}}
<button type="button" {{#if hasNoPhone}}style="display:none;" {{/if}} class="mainbg radius" data-wwid="investigate">Analiza telefon</button>
<button type="button" {{#unless searchLinks}}style="display:none;" {{/unless}} class="mainbg radius" data-wwid="investigate_img">Analiza imagine</button>

{{#if nimfomaneLink}}
    <a href='{{nimfomaneLink}}' target='_blank' style='position: relative'>
        <svg
                style='position: absolute;
               width: 20px;
               height: 20px;
               margin-top: 2px;
               margin-left: 2px;'
                xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16">
            <polygon fill='#8fc38f' points="2.67 7.63 5.46 10.41 13.33 2.54 14.85 4.06 5.46 13.46 1.15 9.15 2.67 7.63"/>
        </svg>
        <img style='background: #2f4979;
                  height: 45px;
                  border-radius: 4px;
                  padding: 12px;'
             src='https://nimfomane.com/forum/uploads/monthly_2024_01/logo2.png.d461ead015b3eddf20bc0a9b70668583.png'/>
    </a>
{{/if}}

{{#if dueToPhoneHidden}}
    <div style="margin-top: 5px">
        <p style="color: rgb(34, 34, 34); font-size: .8125rem;">acest nr de telefon deja a fost marcat ascuns de la alt anunț</p>
    </div>
{{/if}}

{{#if visible }}
    {{#if searchLinks}}
        <div>
            <h4 style='font-weight: bold; padding-top: 10px;'>{{phone}}</h4>
            <h5 style="margin-top: 5px; margin-bottom: 3px; font-size: 1.125rem">Rezultate după telefon</h5>
            {{#if (isUndefined searchLinks)}}
                <p style="color: rgb(34, 34, 34); font-size: .8125rem;">nerulat</p>
            {{else}}
                {{#unless filteredSearchLinks.length}}
                    <p style="color: rgb(103,109,92); font-size: .8125rem;">nu au fost găsite linkuri relevante</p>
                {{else}}
                    {{#each filteredSearchLinks}}
                        <div style="font-size: .8125rem; text-overflow: ellipsis; white-space: nowrap; overflow: hidden;">
                            <a onclick='event.stopPropagation()' href="{{this}}" target="_blank">{{this}}</a></div>
                    {{/each}}
                {{/unless}}
            {{/if}}

            <h5 style="margin-top: 5px; margin-bottom: 3px; font-size: 1.125rem">Rezultate dupa imagine</h5>
            {{#unless imageSearchDomains.length}}
                <p style="color: rgb(34, 34, 34); font-size: .8125rem;">nerulat</p>
            {{else}}
                {{#unless imageSearchDomains.length}}
                    <p style="color: rgb(103,109,92); font-size: .8125rem;">nu au fost găsite linkuri relevante</p>
                {{else}}
                    <div style="font-size: .8125rem;">
                        {{#each imageSearchDomains}}
                            {{#each this.links}}
                                <a href="{{this}}" target="_blank"
                                   style="display: inline-block;
                                           background-color: {{#if ../isSafe}}#d1e1d1{{else}}#efe2e2{{/if}};
                                           padding: 5px;
                                           margin-right: 3px;
                                           margin-bottom: 6px;
                                           border-radius: 5px;
                                           text-decoration: none;
                                           color: rgb(55 55 55);
                               ">
                                    {{#if @first}}
                                        {{../domain}}
                                    {{else}}
                                        <span style="font-size: .6rem">#</span>{{inc @index}}
                                    {{/if}}
                                </a>
                            {{/each}}
                        {{/each}}
                    </div>
                {{/unless}}
            {{/unless}}
        </div>
    {{else}}
        <span>neanalizat</span>
    {{/if}}
{{else }}

{{/if}}
