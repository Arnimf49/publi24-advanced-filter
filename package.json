{
  "name": "publi24-advanced-filter",
  "type": "module",
  "version": "1.0.0",
  "description": "https://chromewebstore.google.com/detail/publi24-advanced-filter/pigkjfndnpblohnmphgbmecaelefaedn?authuser=0&hl=en",
  "main": "index.js",
  "scripts": {
    "build": "npm run rollup-b && web-ext build --ignore-files \"tests\" \"test-results\" \"misc\" \"src\" \"package.json\" \"package-lock.json\" \"playwright.config.ts\" \"node_modules\" \"tsconfig.json\" \"rollup.config.js\" --overwrite-dest && git archive --format=zip --output ./web-ext-artifacts/publi24-advanced-filter--main.zip main",
    "rollup-b": "rollup -c",
    "rollup": "rollup -c -w",
    "pw-open": "npx playwright test --ui",
    "pw-debug": "npx playwright test --debug",
    "pw-run": "npx playwright test",
    "postinstall": "sed -i 's/:visible//g' node_modules/recaptcha-solver/lib/index.js node_modules/recaptcha-solver/lib/index.mjs || true"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.51.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/chrome": "^0.0.313",
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "cheerio": "^1.0.0",
    "cssnano": "^7.0.6",
    "execa": "^9.5.2",
    "postcss": "^8.5.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recaptcha-solver": "^0.3.0",
    "rollup": "^4.39.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.86.3",
    "typescript": "^5.8.3"
  }
}
