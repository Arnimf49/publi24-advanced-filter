{
  "name": "publi24-advanced-filter",
  "type": "module",
  "version": "1.0.0",
  "description": "https://chromewebstore.google.com/detail/publi24-advanced-filter/pigkjfndnpblohnmphgbmecaelefaedn?authuser=0&hl=en",
  "main": "index.js",
  "scripts": {
    "build": "./misc/build.sh",
    "rollup-b": "rollup -c",
    "rollup": "rollup -c -w",
    "pw-open": "npx playwright test --ui",
    "pw-debug": "npx playwright test --debug",
    "pw-run": "npx playwright test",
    "show-domains": "node misc/show-unknown-domains.js",
    "clean-unknown-domains": "node src-mapper/clean-unknown-domains.js",
    "domain-mapper:server": "node src-mapper/server.js",
    "domain-mapper:ui": "vite --config src-mapper/vite.config.js",
    "domain-mapper": "concurrently \"npm run domain-mapper:server\" \"npm run domain-mapper:ui\"",
    "postinstall": "sed -i 's/:visible//g' node_modules/recaptcha-solver/lib/index.js node_modules/recaptcha-solver/lib/index.mjs || true"
  },
  "author": "",
  "license": "MIT",
